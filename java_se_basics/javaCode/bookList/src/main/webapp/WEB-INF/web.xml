<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>cs</servlet-name>
    <servlet-class>com.jk.CartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ls</servlet-name>
    <servlet-class>com.jk.ListBookServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ps</servlet-name>
    <servlet-class>com.jk.PurchaseServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>cs</servlet-name>
    <url-pattern>/servlet/CartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ls</servlet-name>
    <url-pattern>/servlet/ListBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ps</servlet-name>
    <url-pattern>/servlet/PurchaseServlet</url-pattern>
  </servlet-mapping>
</web-app>
